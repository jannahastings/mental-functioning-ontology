    <!-- http://purl.obolibrary.org/obo/MF_0000000 -->

    <owl:Class rdf:about="&obo;MF_0000000">
        <rdfs:label rdf:datatype="&xsd;string">Mental Functioning Related Anatomical Structure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000007"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#inhererOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000001"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>ANATOMICAL STRUCTURE in which there inheres the DISPOSITION to be the agent
of a MENTAL PROCESS.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000001 -->

    <owl:Class rdf:about="&obo;MF_0000001">
        <rdfs:label rdf:datatype="&xsd;string">Disposition to be agentOf Mental Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;Disposition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#inheresIn"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000000"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000002 -->

    <owl:Class rdf:about="&obo;MF_0000002">
        <rdfs:label rdf:datatype="&xsd;string">Disposition to lead to Pathological Mental Processes</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000006"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000003 -->

    <owl:Class rdf:about="&obo;MF_0000003">
        <rdfs:label rdf:datatype="&xsd;string">Behavior Inducing State</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000009"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#participantOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000022"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#inheresIn"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000000"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>BODILY QUALITY inhering in a MENTAL
FUNCTIONING RELATED ANATOMICAL STRUCTURE which leads to BEHAVIOR of some
specific sort.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000004 -->

    <owl:Class rdf:about="&obo;MF_0000004">
        <rdfs:label rdf:datatype="&xsd;string">Mental Disorder</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000010"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#partOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000000"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>DISORDER in a MENTAL FUNCTIONING
RELATED ANATOMICAL STRUCTURE.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000005 -->

    <owl:Class rdf:about="&obo;MF_0000005">
        <rdfs:label rdf:datatype="&xsd;string">Mental Disease</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000002"/>
        <obo:IAO_0000115>a DISEASE which is a DISPOSITION to undergo
PATHOLOGICAL MENTAL PROCESSES.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000006 -->

    <owl:Class rdf:about="&obo;MF_0000006">
        <rdfs:label rdf:datatype="&xsd;string">Disposition to lead to Pathological Processes</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;Disposition"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000007 -->

    <owl:Class rdf:about="&obo;MF_0000007">
        <rdfs:label rdf:datatype="&xsd;string">Anatomical Structure</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;IndependentContinuant"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#partOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000015"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000009 -->

    <owl:Class rdf:about="&obo;MF_0000009">
        <rdfs:label rdf:datatype="&xsd;string">Bodily Quality</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;Quality"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#inheresIn"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000012"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>QUALITY inhering in a BODILY COMPONENT.
Source:
http://www.jbiomedsem.com/content/1/1/10.</obo:IAO_0000115>
        <rdfs:comment>(from OGMS)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000010 -->

    <owl:Class rdf:about="&obo;MF_0000010">
        <rdfs:label rdf:datatype="&xsd;string">Disorder</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000012"/>
        <obo:IAO_0000115>A combination of BODILY COMPONENTS of or in
an ORGANISM that is clinically abnormal.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
        <rdfs:comment>(from OGMS)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000011 -->

    <owl:Class rdf:about="&obo;MF_0000011">
        <rdfs:label rdf:datatype="&xsd;string">Disease</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000006"/>
        <obo:IAO_0000115>a DISPOSITION (i) to undergo PATHOLOGICAL
PROCESSES that (ii) exists in an ORGANISM
because of one or more DISORDERS in that
ORGANISM.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
        <rdfs:comment>(from OGMS)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000012 -->

    <owl:Class rdf:about="&obo;MF_0000012">
        <rdfs:label rdf:datatype="&xsd;string">Bodily Component</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000014"/>
        <obo:IAO_0000115>MATERIAL OBJECT within or on the surface of an
ORGANISM, including ANATOMICAL
STRUCTURES, body flora, pathogens, toxins, and
their combinations, potentially including also the
organism as a whole.
Source: http://www.jbiomedsem.com/content/1/1/10.</obo:IAO_0000115>
        <rdfs:comment>(from OGMS)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000014 -->

    <owl:Class rdf:about="&obo;MF_0000014">
        <rdfs:label rdf:datatype="&xsd;string">Material Object</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;IndependentContinuant"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000015 -->

    <owl:Class rdf:about="&obo;MF_0000015">
        <rdfs:label rdf:datatype="&xsd;string">Organism</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000014"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000016 -->

    <owl:Class rdf:about="&obo;MF_0000016">
        <rdfs:label rdf:datatype="&xsd;string">Human Being</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000015"/>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000020 -->

    <owl:Class rdf:about="&obo;MF_0000020">
        <rdfs:label rdf:datatype="&xsd;string">Mental Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000026"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#realizationOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000001"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasParticipant"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000003"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>BODILY PROCESS which brings into being,
sustains or modifies a COGNITIVE REPRESENTATION or a BEHAVIOR INDUCING
STATE.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000021 -->

    <owl:Class rdf:about="&obo;MF_0000021">
        <rdfs:label rdf:datatype="&xsd;string">Interpersonal Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&span;Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasParticipant"/>
                <owl:onClass rdf:resource="&obo;MF_0000016"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>PROCESS in which at least two HUMAN
BEINGS are agents.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000022 -->

    <owl:Class rdf:about="&obo;MF_0000022">
        <rdfs:label rdf:datatype="&xsd;string">Behavior</rdfs:label>
        <rdfs:subClassOf rdf:resource="&span;Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasAgent"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000015"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>a PROCESS having PROCESSES as parts in which an
ORGANISM participates as agent in response to external or internal stimuli and
following some pattern which is dependent upon some combination of that
ORGANISM’s internal state and external conditions. (Derived from the Gene
Ontology).
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000023 -->

    <owl:Class rdf:about="&obo;MF_0000023">
        <rdfs:label rdf:datatype="&xsd;string">Interpersonal Behavior</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000022"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasPart"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000021"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>BEHAVIOR having INTERPERSONAL
PROCESSES as parts, each involving the same instances of HUMAN BEING.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000024 -->

    <owl:Class rdf:about="&obo;MF_0000024">
        <rdfs:label rdf:datatype="&xsd;string">Pathological Mental Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000020"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#manifestationOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000004"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>PATHOLOGICAL PROCESS
which is the manifestation of a MENTAL DISORDER.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000025 -->

    <owl:Class rdf:about="&obo;MF_0000025">
        <rdfs:label rdf:datatype="&xsd;string">Mental Disease Course</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000029"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasPart"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000024"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>DISEASE COURSE of a MENTAL DISEASE.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000026 -->

    <owl:Class rdf:about="&obo;MF_0000026">
        <rdfs:label rdf:datatype="&xsd;string">Bodily Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&span;Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasParticipant"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000012"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>PROCESS in which at least one BODILY
COMPONENT of an ORGANISM participates.
Source:
http://www.jbiomedsem.com/content/1/1/10
</obo:IAO_0000115>
        <rdfs:comment>(from OGMS)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000028 -->

    <owl:Class rdf:about="&obo;MF_0000028">
        <rdfs:label rdf:datatype="&xsd;string">Pathological Process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000026"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#manifestationOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000010"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>(from OGMS)</rdfs:comment>
        <obo:IAO_0000115>a BODILY PROCESS that is a manifestation of a
DISORDER.
Source:
http://www.jbiomedsem.com/content/1/1/10
</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000029 -->

    <owl:Class rdf:about="&obo;MF_0000029">
        <rdfs:label rdf:datatype="&xsd;string">Disease Course</rdfs:label>
        <rdfs:subClassOf rdf:resource="&span;Process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#hasPart"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000028"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>(from OGMS)</rdfs:comment>
        <obo:IAO_0000115>the totality of all PROCESSES through which a
given DISEASE instance is realized.
Source:
http://www.jbiomedsem.com/content/1/1/10

</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000030 -->

    <owl:Class rdf:about="&obo;MF_0000030">
        <rdfs:label rdf:datatype="&xsd;string">Representation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&snap;DependentContinuant"/>
        <obo:IAO_0000115>DEPENDENT CONTINUANT which is about a
portion of reality.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000031 -->

    <owl:Class rdf:about="&obo;MF_0000031">
        <rdfs:label rdf:datatype="&xsd;string">Cognitive Representation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000030"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#specificallyDependsOn"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000007"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>REPRESENTATION which
specifically depends on an ANATOMICAL STRUCTURE in the cognitive system of an
ORGANISM.</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000032 -->

    <owl:Class rdf:about="&obo;MF_0000032">
        <rdfs:label rdf:datatype="&xsd;string">Observation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000031"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#isAbout"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000009"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>(from OGMS)</rdfs:comment>
        <obo:IAO_0000115>COGNITIVE REPRESENTATION about a BODILY
FEATURE resulting from an act of perception.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000033 -->

    <owl:Class rdf:about="&obo;MF_0000033">
        <rdfs:label rdf:datatype="&xsd;string">Diagnosis</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000030"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#isAbout"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000011"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>REPRESENTATION asserting the presence of an
instance of DISEASE in a given ORGANISM
resulting from an interpretive process that has as
input a CLINICAL PICTURE of that ORGANISM.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
        <rdfs:comment>(from OGMS)</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000034 -->

    <owl:Class rdf:about="&obo;MF_0000034">
        <rdfs:label rdf:datatype="&xsd;string">Disease Picture</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000030"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#isAbout"/>
                <owl:someValuesFrom rdf:resource="&obo;MF_0000042"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <obo:IAO_0000115>REPRESENTATION of a DISEASE PHENOTYPE
concretized in some information artifact.
Source:
http://www.jbiomedsem.com/content/1/1/10</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MF_0000035 -->

    <owl:Class rdf:about="&obo;MF_0000035">
