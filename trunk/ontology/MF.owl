    <!-- http://purl.obolibrary.org/obo/MF_0000008 -->

    <owl:Class rdf:about="&obo;MF_0000008">
        <rdfs:label>cognitive process</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000020"/>
