    <!-- http://purl.obolibrary.org/obo/MF_0000044 -->

    <owl:Class rdf:about="&obo;MF_0000044">
        <rdfs:label>sensory perception</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000019"/>
        <rdfs:comment>This branch is (to be) MIREOTED in from the Gene Ontology (GO). IDs are preserved. If, at a later stage, a need emerges to maintain this branch separately within MFO, special care will need to be taken to coordinate the ID space in the branch. </rdfs:comment>
