    <!-- http://purl.obolibrary.org/obo/MD_0000036 -->

    <owl:Class rdf:about="&obo;MD_0000036">
        <rdfs:label rdf:datatype="&xsd;string">disease picture component</rdfs:label>
        <rdfs:subClassOf rdf:resource="&obo;MF_0000035"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&obo;MF#partOf"/>
                <owl:someValuesFrom rdf:resource="&obo;MD_0000034"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
